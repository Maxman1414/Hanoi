<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXListView?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="269.0" prefWidth="518.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <HBox alignment="CENTER" prefHeight="88.0" prefWidth="600.0">
         <children>
            <JFXButton fx:id="backBtn" buttonType="RAISED" disable="true" focusTraversable="false" onAction="#backPressed" prefHeight="44.0" prefWidth="99.0" text="Back" />
            <JFXButton fx:id="playBtn" buttonType="RAISED" focusTraversable="false" onAction="#playPressed" prefHeight="46.0" prefWidth="69.0" text="Play" />
            <JFXButton fx:id="nextBtn" buttonType="RAISED" focusTraversable="false" onAction="#nextPressed" prefHeight="44.0" prefWidth="104.0" text="Next" />
         </children>
      </HBox>
      <Label fx:id="moveLabel" alignment="CENTER" focusTraversable="false" maxWidth="1.7976931348623157E308" minWidth="-Infinity" prefHeight="17.0" text="Start" textAlignment="CENTER" />
      <JFXListView fx:id="moveView" onKeyReleased="#selectionMade" onMouseReleased="#selectionMade" prefHeight="260.0" prefWidth="600.0" />
   </children>
</VBox>
